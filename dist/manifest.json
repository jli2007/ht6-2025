{
  "id": "ht6-adobe",
  "name": "ht6-adobe",
  "version": "1.0.0",
  "main": "index.html",
  "manifestVersion": 5,
  "host": [
    {
      "app": "PS",
      "minVersion": "26.8.1"
    }
  ],
  "requiredPermissions": {
    "allowCodeGenerationFromStrings": true,
    "launchProcess": {
      "schemes": [
        "http",
        "https"
      ],
      "extensions": [
        ".svg",
        ".png"
      ]
    },
    "network": {
      "domains": [
        "https://generativelanguage.googleapis.com",
        "https://*.googleapis.com"
      ]
    },
    "webview": {
      "allow": "yes",
      "domains": [
        "https://*.adobe.com",
        "https://*.google.com"
      ]
    },
    "localFileSystem": "fullAccess"
  },
  "entrypoints": [
    {
      "type": "command",
      "id": "showAbout",
      "label": {
        "default": "About React Demo"
      },
      "shortcut": {
        "mac": "shift+cmd+A",
        "win": "ctrl+alt+A"
      }
    },
    {
      "type": "panel",
      "id": "demos",
      "label": {
        "default": "Demo Panel"
      },
      "minimumSize": {
        "width": 230,
        "height": 200
      },
      "maximumSize": {
        "width": 2000,
        "height": 2000
      },
      "preferredDockedSize": {
        "width": 230,
        "height": 300
      },
      "preferredFloatingSize": {
        "width": 230,
        "height": 300
      }
    }
  ]
}